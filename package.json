{
  "name": "nextops-ai",
  "private": true,
  "packageManager": "npm@10.9.4",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "turbo": "^2.8.3"
  }
}
